{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    },
    {
      "src": "prisma/schema.prisma",
      "use": "@prisma/cli",
      "config": { "generation": { "output": "prisma/client" } }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}